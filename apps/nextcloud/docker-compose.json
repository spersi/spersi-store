{
	"services":[
		{
			"name": "nextcloud",
			"image": "nextcloud:latest",
			"isMain": true,
			"internalPort": 80,
            "volumes": [
                {
                  "hostPath": "${APP_DATA_DIR}/data/nextcloud:/var/www/html",
                  "containerPath": "data",
                  "readOnly": false
                }
            ]
		}
	]
}
